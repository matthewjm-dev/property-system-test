<div id="main" class="page page-{{ class }}">
	<section>
		<div class="container">
			<div class="row">
                <div class="col-md-12">
                    <h1>{{ title }}</h1>
                    <div class="dyn-content">
                        {{ content|raw }}
                    </div>
					<div id="contact-details">
						<p>{{ contact_email_text }} <a href="mailto:{{ contact_email }}" target="_blank">{{ contact_email }}</a></p>
						<p>{{ contact_phone_text }} <a href="tel:{{ contact_phone }}" target="_blank">{{ contact_phone }}</a></p>
					</div>
                </div>
			</div>
		</div>
	</section>
	<section id="section-contact-form">
		<div class="container">
			<div class="row">
				<div class="col-sm-1 col-md-2 col-lg-3"></div>
				<div class="col-sm-10 col-md-8 col-lg-6">
					{{ contact_form|raw }}
				</div>
				<div class="col-sm-1 col-md-2 col-lg-3"></div>
			</div>
		</div>
	</section>
</div>
