<div id="main" class="page-service">
    <section id="service-head">
        <div class="container">
            <div class="row">
                <div class="col-9 col-md-10">
                    <h1>{{ title }}</h1>
                </div>
                <div class="col-3 col-md-2">
                    <div id="service-icon">
                        <i class="{{ icon }}"></i>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="dyn-content">
                        {{ content|raw }}
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>
